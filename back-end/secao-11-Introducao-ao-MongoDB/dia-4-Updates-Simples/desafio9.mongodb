//Utilize o operador $max para alterar o imdbRating para 8.6 do filme Godzilla. Al√©m disso, altere a categoria "adventure" para "thriller" do filme Godzilla.

use('cinema');

db.movies.updateOne(
    { title: "Godzilla" },
    { 
        $max: { imdbRating: 8.6 }, 
        $set: { "category.1": "thriller" }
    },
);